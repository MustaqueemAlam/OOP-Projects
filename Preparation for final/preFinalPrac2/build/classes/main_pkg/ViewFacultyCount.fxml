<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="654.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="main_pkg.ViewFacultyCountController">
   <children>
      <TableView fx:id="tableView" layoutX="27.0" layoutY="60.0" prefHeight="89.0" prefWidth="559.0">
        <columns>
          <TableColumn fx:id="DepartmentColumn" prefWidth="330.0" text="Department" />
          <TableColumn fx:id="FacultyCountCol" prefWidth="228.0" text="Faculty Count " />
        </columns>
      </TableView>
      <ComboBox fx:id="departmentcombBox" layoutX="436.0" layoutY="23.0" onAction="#departmentcombBoxOnClick" prefWidth="150.0" promptText="Select Department" />
      <PieChart fx:id="piechart" layoutX="27.0" layoutY="167.0" prefHeight="246.0" prefWidth="292.0" title="Piechart" />
      <BarChart fx:id="barchart" layoutX="331.0" layoutY="175.0" prefHeight="231.0" prefWidth="301.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
