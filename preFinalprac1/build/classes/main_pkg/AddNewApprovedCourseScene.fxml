<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="305.0" prefWidth="344.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main_pkg.AddNewApprovedCourseController">
    <children>
        <Label layoutX="140" layoutY="20" text="Add Approved Course" />
        <Label layoutX="20" layoutY="60" text="Course ID:" />
        <TextField fx:id="courseIdTextField" layoutX="150" layoutY="55" />
        <Label layoutX="20" layoutY="100" text="Title:" />
        <TextField fx:id="titleTextField" layoutX="150" layoutY="95" />
        <Label layoutX="20" layoutY="140" text="Number of Credits:" />
        <TextField fx:id="noOfCreditsTextField" layoutX="150" layoutY="135" />
        <Label layoutX="20" layoutY="180" text="Course Type:" />
        <TextField fx:id="courseTypeTextField" layoutX="150" layoutY="175" />
        <Label layoutX="20" layoutY="220" text="Course Category:" />
        <TextField fx:id="courseCategoryTextField" layoutX="150" layoutY="215" />
        <Button layoutX="150" layoutY="260" onAction="#addNewCourse" text="Add Approved Course" />
    </children>
</AnchorPane>
