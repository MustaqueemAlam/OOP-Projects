      </Button>
      <TextArea fx:id="textArea" layoutX="49.0" layoutY="95.0" prefHeight="275.0" prefWidth="503.0" />
      <Button fx:id="backButton" layoutX="315.0" layoutY="24.0" mnemonicParsing="false" onAction="#backButtonOnClick" prefHeight="31.0" prefWidth="233.0" style="-fx-background-radius: 88;" text="Back">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
