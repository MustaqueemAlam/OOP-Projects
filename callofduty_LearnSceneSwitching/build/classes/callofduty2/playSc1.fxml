<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="334.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="callofduty2.PlaySc1Controller">
   <children>
      <ImageView fitHeight="340.0" fitWidth="597.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/codsc3.jpg" />
         </image>
      </ImageView>
      <Label fx:id="outputlabel" layoutX="137.0" layoutY="118.0" prefHeight="49.0" prefWidth="351.0" style="-fx-background-color: white;" />
      <Button fx:id="PauseButton" layoutX="493.0" layoutY="60.0" mnemonicParsing="false" onAction="#PauseButtonOnClick" prefHeight="31.0" prefWidth="67.0" style="-fx-background-color: blue;" text="Pause" textFill="WHITE">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Button>
      <Button fx:id="climbButton" layoutX="33.0" layoutY="172.0" mnemonicParsing="false" onAction="#climbButtonOnClick" text="Climb">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="CollectWeaponButton" layoutX="21.0" layoutY="32.0" mnemonicParsing="false" onAction="#CollectWeaponButtonOnClick" text="Collect Weapon">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="attackButton" layoutX="33.0" layoutY="228.0" mnemonicParsing="false" onAction="#attackButtonOnClick" prefHeight="73.0" prefWidth="126.0" text="Attack">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <ButtonBar layoutX="138.0" layoutY="183.0" prefHeight="40.0" prefWidth="200.0" />
      <Label fx:id="lifebarLabel" layoutX="313.0" layoutY="52.0" prefHeight="23.0" prefWidth="67.0" style="-fx-background-color: white;" />
      <Label layoutX="208.0" layoutY="54.0" prefHeight="20.0" prefWidth="104.0" text="Remaining Life:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="StopPlayButton" layoutX="479.0" layoutY="18.0" mnemonicParsing="false" onAction="#StopPlayButtonOnClick" style="-fx-background-color: red;" text="Stop Playing" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="RunButton" layoutX="514.0" layoutY="139.0" mnemonicParsing="false" onAction="#RunButtonOnClick" prefHeight="73.0" prefWidth="43.0" text="Run">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="walkButton" layoutX="456.0" layoutY="240.0" mnemonicParsing="false" onAction="#walkButtonOnClick" prefHeight="25.0" prefWidth="74.0" text="Walk">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="CrawlButton" layoutX="424.0" layoutY="281.0" mnemonicParsing="false" onAction="#CrawlButtonOnClick" prefHeight="20.0" prefWidth="146.0" text="Crawl">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
