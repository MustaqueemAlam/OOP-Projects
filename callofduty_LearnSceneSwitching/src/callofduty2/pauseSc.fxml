            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="ExitButton2" layoutX="64.0" layoutY="135.0" mnemonicParsing="false" onAction="#Exit2ButtonOnClick" prefHeight="34.0" prefWidth="115.0" text="Exit">
