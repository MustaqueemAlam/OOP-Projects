<AnchorPane id="AnchorPane" fx:id="scenePane2" prefHeight="250.0" prefWidth="485.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="callofduty2.PauseScController">
   <children>
      <ImageView fitHeight="231.0" fitWidth="476.0" layoutX="13.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
